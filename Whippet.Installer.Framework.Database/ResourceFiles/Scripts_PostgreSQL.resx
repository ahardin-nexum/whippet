<?xml version="1.0" encoding="utf-8"?>

<root>
  <xsd:schema id="root" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xsd:element name="root" msdata:IsDataSet="true">
      <data name="DB_CREATE" type="System.String, mscorlib">
        <value>
          CREATE DATABASE $$WHIPPET$$ 
            WITH 
              OWNER = 'whippet_sa' 
              ENCODING = 'UTF8' 
              LC_COLLATE = 'en_US.utf8' 
              LC_CTYPE = 'en_US.utf8' 
              LOCALE_PROVIDER = 'libc' 
              TABLESPACE = pg_default 
              CONNECTION LIMIT = -1 
              IS_TEMPLATE = False;          
        </value>
      </data>
      <data name="DB_LOGIN" type="System.String, mscorlib">
        <value>
            CREATE ROLE whippet_sa WITH
              LOGIN
              NOSUPERUSER
              INHERIT
              CREATEDB
              NOCREATEROLE
              NOREPLICATION
              PASSWORD '$$PASSWORD$$'
        </value>
      </data>
      <data name="DB_SCHEMA" type="System.String, mscorlib">
        <value>
          CREATE SCHEMA whippet
            AUTHORIZATION whippet_sa;          
        </value>
      </data>
    </xsd:element>
  </xsd:schema>
